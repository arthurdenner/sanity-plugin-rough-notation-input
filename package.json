  "author": "Arthur Denner <arthurdenner7@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "sanipack": "^2.1.0",
    "prettier": "^2.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-react": "^7.28.0"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "bugs": {
    "url": "https://github.com/arthurdenner/sanity-plugin-rough-notation-input/issues"
